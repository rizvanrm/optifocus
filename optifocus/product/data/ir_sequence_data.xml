<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="seq_product_barcode" model="ir.sequence">
        <field name="name">Product Barcode</field>
        <field name="code">product.barcode</field>
        <field name="padding">6</field>
        <field name="number_next">100000</field>
        <field name="company_id" eval="False"/>
    </record>



    <record id="seq_product_brand" model="ir.sequence">
        <field name="name">Product Brand</field>
        <field name="code">product.brand</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False"/>
    </record>


    <record id="seq_product_category" model="ir.sequence">
        <field name="name">Product Category</field>
        <field name="code">product.category</field>
        <field name="padding">0</field>
        <field name="company_id" eval="False"/>
    </record>

</odoo>
